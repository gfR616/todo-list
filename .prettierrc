{
	"printWidth": 90,
	"singleQuote": true,
	"trailingComma": "all",
	"bracketSpacing": true,
	"bracketSameLine": false,
	"tabWidth": 2,
	"semi": false,
	"useTabs": true,
	"arrowParens": "always",
	"endOfLine": "auto",
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true,
	"plugins": ["@trivago/prettier-plugin-sort-imports"]
}
